fun int write_int(int x) = write(x)
fun char write_char(char x) = write(x)
fun bool write_bool(bool x) = write(x)

fun [int] main() =
  let n = read(int) in
  let tmp = map(write_bool, replicate(n, true)) in
  let tmp = write("\n") in
  let tmp = map(write_int, replicate(n, 6)) in
  let tmp = write("\n") in
  let tmp = map(write_char, replicate(n, 'a')) in
  iota(n)
